{
  "version": 2,
  "name": "blogwebsite",
  "builds": [{
    "src": "app.js",
    "use": "@vercel/node"
  }],
  "routes": [
    { "src": "/styles/(.*)", "dest": "/public/styles/$1" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" },
    { "src": "/main.js", "dest": "/public/main.js" },
    { "src": "/(.*)", "dest": "app.js" }
  ]
}
